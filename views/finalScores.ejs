<div id="FinalScore stage">
    <div class="input-container">
        <h2>Final Score</h2>
    </div>

    <div class="scoreList-container">
        <!-- 三个列表的容器 -->
        <div class="scrol-lists-container">
            <!-- 左侧滚动列表 -->
            <div class="scrol-list-wrapper">
                <h3 class="list-title silver-title">Silver</h3> <!-- 左侧标题 -->
                <ul class="scrol-list left-list">
                    <li v-for="(item, index) in silver" :key="`list1-${index}`">
                        {{ item }}
                    </li>
                </ul>
            </div>

            <!-- 中间滚动列表 -->
            <div class="scrol-list-wrapper">
                <h3 class="list-title gold-title">Gold</h3> <!-- 中间标题 -->
                <ul class="scrol-list middle-list">
                    <li v-for="(item, index) in gold" :key="`list3-${index}`">
                        {{ item }}
                    </li>
                </ul>
            </div>

            <!-- 右侧滚动列表 -->
            <div class="scrol-list-wrapper">
                <h3 class="list-title bronze-title">Bronze</h3> <!-- 右侧标题 -->
                <ul class="scrol-list right-list">
                    <li v-for="(item, index) in bronze" :key="`list2-${index}`">
                        {{ item }}
                    </li>
                </ul>
            </div>
        </div>
    </div>

    <div class="buttonRight-container">
        <button @click="thisGameEnd">Start the New Game</button>
    </div>
</div>